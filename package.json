{
  "name": "finances-report",
  "version": "1.0.0",
  "description": "finances robot",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node ./src/infra/index.ts",
    "prismaa": "prisma generate --schema=./src/infra/adapters/repositories/prisma/schema.prisma ",
    "clean:install": "rm -rf node_modules && rm -f package-lock.json && rm -f yarn.lock && rm -rf build && mkdir build && npm i ",
    "build": "npm run clean:install && npx tsc --build && npm run prismaa &&  node build/infra/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "html-pdf": "^3.0.1",
    "json2xls": "^0.1.2",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.5",
    "prisma": "^4.1.0",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/html-pdf": "^3.0.0",
    "@types/node": "^18.0.0",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.4"
  }
}
